 {
   "manifest_version": 3,
   "name": "PhishGuard Click Interceptor",
   "version": "1.0",
   "permissions": ["scripting", "storage"],
   "host_permissions": ["<all_urls>"],
   "content_scripts": [
     {
       "matches": ["<all_urls>"],
       "js": ["content.js"],
       "run_at": "document_start"
     }
   ],
   "background": { "service_worker": "background.js" },
   "action": {}
 }
